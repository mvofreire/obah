{
  "name": "api",
  "version": "1.1.2",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "serve": "cross-env NODE_ENV=production node dist/server.js",
    "start": "cross-env NODE_ENV=dev nodemon -L --exec babel-node application/server.js",
    "clear": "rimraf dist",
    "test": "echo 'No Tests Found!'",
    "build": "yarn clear && ./node_modules/.bin/babel ./application --out-dir ./dist --source-maps --copy-files",
    "deploy": "netlify deploy --dir=./build -p -m \"$(git log -1 --pretty=%B)\""
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "bcrypt": "^3.0.5",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.4.22",
    "mongoose-paginate-v2": "^1.0.24",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "jszip-cli": "^1.4.24",
    "netlify-cli": "^2.11.22",
    "nodemon": "^1.18.10",
    "rimraf": "^2.6.3"
  }
}
